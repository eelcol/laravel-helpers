{
    "name": "eelcol/laravel-helpers",
    "description": "Laravel helper functions.",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Eelcol\\LaravelHelpers\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eelcol\\LaravelHelpers\\HelpersServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
