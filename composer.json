{
    "name": "eelcol/laravel-helpers",
    "description": "Laravel helper functions.",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3.0",
        "illuminate/support": "^6|^7|^8",
        "illuminate/http": "^6|^7|^8"
    },
    "autoload": {
        "psr-4": {
            "Eelcol\\LaravelHelpers\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Eelcol\\LaravelHelpers\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eelcol\\LaravelHelpers\\HelpersServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.5"
    }
}
